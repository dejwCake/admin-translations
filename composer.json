{
    "name": "dejwcake/admin-translations",
    "description": "Translation manager for brackets/admin-ui",
    "keywords": [
        "translations",
        "localizations",
        "manager",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "David Běhal",
            "email": "david.behal@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "Richard Dominik",
            "email": "richard.dominik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3.0|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "dejwcake/admin-ui": "dev-main",
        "dejwcake/admin-listing": "dev-main",
        "dejwcake/craftable-translatable": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0|~6.0|~7.0",
        "phpunit/phpunit": "^8.5|^9.0",
        "doctrine/dbal": "~2.5.13|^2.6",
        "mockery/mockery": "^1.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dejwCake/admin-listing.git"
        },
        {
            "type": "git",
            "url": "https://github.com/dejwCake/admin-ui.git"
        },
        {
            "type": "git",
            "url": "https://github.com/dejwCake/craftable-translatable.git"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Brackets\\AdminTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\AdminTranslations\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\AdminTranslations\\AdminTranslationsServiceProvider"
            ]
        }
    }
}
